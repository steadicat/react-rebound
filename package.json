{
  "name": "react-rebound",
  "version": "0.6.1",
  "description": "A React component that animates its child using springs.",
  "author": "Stefano J. Attardi (http://github.com/steadicat)",
  "main": "index.js",
  "jsnext:main": "index.es2015.js",
  "scripts": {
    "lint": "eslint src",
    "build": "./node_modules/.bin/rollup -c -f es6 -o index.es2015.js && ./node_modules/.bin/rollup index.es2015.js -f umd -n ReactRebound --globals stylistic:Stylistic -o index.js",
    "prepublish": "npm run build"
  },
  "homepage": "https://github.com/steadicat/react-rebound",
  "repository": {
    "type": "git",
    "url": "git@github.com:steadicat/react-rebound.git"
  },
  "bugs": {
    "url": "https://github.com/steadicat/react-rebound/issues"
  },
  "keywords": [
    "react",
    "animation",
    "animate",
    "springs",
    "rebound",
    "motion"
  ],
  "dependencies": {
    "rebound": "0.0.x",
    "stylistic": "0.2.x"
  },
  "peerDependencies": {
    "react": "0.14.x || >15.0.0-rc",
    "react-dom": "0.14.x || >15.0.0-rc"
  },
  "devDependencies": {
    "babel-eslint": "6.0.0",
    "babel-preset-es2015-rollup": "1.2.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "2.5.3",
    "eslint-plugin-babel": "3.1.0",
    "eslint-plugin-react": "4.2.3",
    "raf": "3.2.0",
    "rollup": "0.25.7",
    "rollup-plugin-babel": "2.4.0",
    "rollup-plugin-commonjs": "2.2.1",
    "rollup-plugin-node-resolve": "1.5.0"
  },
  "license": "MIT"
}
